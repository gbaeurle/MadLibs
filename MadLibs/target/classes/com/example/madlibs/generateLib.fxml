<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" styleClass="pane" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.madlibs.generateController">
   <children>
      <TitledPane fx:id="titlePane" animated="false" collapsible="false" expanded="false" minHeight="-Infinity" minWidth="-Infinity" prefHeight="29.0" prefWidth="600.0" styleClass="header" text="somethingLib">
         <font>
            <Font size="14.0" />
         </font>
      </TitledPane>
      <TextArea fx:id="libText" editable="false" layoutY="200.0" prefHeight="200.0" prefWidth="600.0" styleClass="story" text="Your Mad Lib Will Generate Here!" />
      <ListView fx:id="listWords" layoutY="29.0" prefHeight="172.0" prefWidth="248.0" styleClass="prompt" />
      <TextField fx:id="enterWords" layoutX="348.0" layoutY="63.0" styleClass="inputbox" />
      <Button fx:id="nextWordButton" layoutX="364.0" layoutY="102.0" mnemonicParsing="false" onAction="#nextWord" prefHeight="25.0" prefWidth="128.0" styleClass="next" text="Next" />
      <Button fx:id="replayButton" layoutX="384.0" layoutY="159.0" mnemonicParsing="false" onAction="#toReRun" prefHeight="25.0" prefWidth="88.0" styleClass="again" text="Play Again!" />
   </children>
</AnchorPane>
